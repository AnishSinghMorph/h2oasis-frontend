{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_ROOK_ENVIRONMENT": "sandbox",
        "EXPO_PUBLIC_ROOK_BASE_URL": "https://api.rook-connect.review",
        "EXPO_PUBLIC_OAUTH_REDIRECT_URL": "h2oasis://oauth-callback",
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    },
    "firebase": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ROOK_ENVIRONMENT": "sandbox",
        "EXPO_PUBLIC_ROOK_BASE_URL": "https://api.rook-connect.review",
        "EXPO_PUBLIC_OAUTH_REDIRECT_URL": "h2oasis://oauth-callback",
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    },
    "production": {
      "node": "22.11.0",
      "ios": {
        "buildConfiguration": "Release",
        "scheme": "H2Oasis"
      },
      "env": {
        "EXPO_PUBLIC_ROOK_ENVIRONMENT": "production",
        "EXPO_PUBLIC_ROOK_BASE_URL": "https://api.rook-connect.com",
        "EXPO_PUBLIC_OAUTH_REDIRECT_URL": "h2oasis://oauth-callback",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    }
  },
  "submit": {
    "preview": {
      "android": {
        "serviceAccountKeyPath": "../h2oasis-74d5b-firebase-adminsdk-fbsvc-7668ae4b30.json",
        "track": "internal"
      }
    },
    "production": {}
  }
